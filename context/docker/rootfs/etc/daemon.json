{
  "max-concurrent-downloads": 20,
  "log-driver": "json-file",
  "log-level": "warn",
  "log-opts": {
    "max-size": "10m",
    "max-file": "3"
  },
  "mirror-registries": [
    {
      "domain": "*",
      "mirrors": [
        "https://sea.hub:5000"
      ]
    }
  ],
  "exec-opts": [
    "native.cgroupdriver=systemd"
  ],
  "data-root": "/var/lib/docker",
  "insecure-registries":["0.0.0.0:5000","sea.hub:5000"]
}
